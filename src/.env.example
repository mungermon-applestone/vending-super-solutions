
# Contentful Configuration
# Copy this file to .env in the project root and fill in your values

# Required: Space ID for your Contentful space
VITE_CONTENTFUL_SPACE_ID=your_space_id_here

# Required: Delivery API Token for accessing content
VITE_CONTENTFUL_DELIVERY_TOKEN=your_delivery_token_here

# Optional: Environment ID (defaults to 'master' if not specified)
VITE_CONTENTFUL_ENVIRONMENT_ID=master
